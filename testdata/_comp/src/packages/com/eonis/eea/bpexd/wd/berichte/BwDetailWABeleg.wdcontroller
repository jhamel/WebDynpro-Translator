	//@@begin javadoc:fillEntry()
	/** Declared method. */
	//@@end
	public void fillEntry() {
		//@@begin fillEntry()
		IEntryElement newEntryElement, newPosEntryElement;
		ExportBerichtswesenMaterialaufmassWaBelegGetList export = wdContext.currentOutputBerichtswesenMaterialaufmassWaBelegGetListElement().modelObject();

		BerichtswesenMaterialaufmassWaKopf kopf = (BerichtswesenMaterialaufmassWaKopf) export.getBelegeKopefe().get(0);
		wdContext.nodeEntry().invalidate();
		newEntryElement = wdContext.nodeEntry().createEntryElement();
		wdContext.nodeEntry().addElement(newEntryElement);
		// Kopf
		wdContext.currentEntryElement().setBelegnummer(kopf.getNummerDesMaterialbelegs());
		wdContext.currentEntryElement().setBelegdatum(kopf.getBelegdatumImBeleg());
		SimpleDateFormat sdfDate = new SimpleDateFormat("dd.MM.yyyy");
		SimpleDateFormat sdfTime = new SimpleDateFormat("HH:mm");
		wdContext.currentEntryElement().setErfassungsdatumMitZeit(sdfDate.format(kopf.getTagDerErfassungDesBuchhaltungsbeleges()) + " " + sdfTime.format(kopf.getUhrzeitDerErfassung()));
		wdContext.currentEntryElement().setBelegkopftext(kopf.getBelegkopftext());
		newEntryElement.setIsLeaf(false);

		for (Iterator iter = export.getBelegePositionen().iterator(); iter.hasNext();) {
			newPosEntryElement = newEntryElement.nodeChildEntries_1().createEntryElement();
			newEntryElement.nodeChildEntries_1().addElement(newPosEntryElement);
			BerichtswesenMaterialaufmassWaPos pos = (BerichtswesenMaterialaufmassWaPos) iter.next();
			newPosEntryElement.setPositionsnummer(pos.getMaterialbelegzeile());
			newPosEntryElement.setWerk(pos.getWerk());
			newPosEntryElement.setWaMenge(pos.getMengeInBearbeitungsmengeneinheit());
			newPosEntryElement.setCharge(pos.getChargennummer());
			newPosEntryElement.setLagerort(pos.getLagerort());
			newPosEntryElement.setLagerortText(pos.getLagerortbezeichnung());
			newPosEntryElement.setMaterialbezeichnung(pos.getMaterialkurztext());
			newPosEntryElement.setMaterialnummer(pos.getMaterialnummer());
			newPosEntryElement.setMe(pos.getBearbeitungsmengeneinheit());
			newPosEntryElement.setIsLeaf(true);
		}

		//@@end
	}

